<div class="col-sm-3 sidenav">
  <div class="input-group">
    <input type="text" class="form-control" placeholder="Search for opportunities..">
    <span class="input-group-btn">
      <button class="btn btn-default" type="button">
        <span class="glyphicon glyphicon-search"></span>
      </button>
    </span>
  </div>
  <ul class="nav nav-pills nav-stacked">
      <li class="active"><a data-toggle="pill" href="#profile">Profile</a></li>
      <li><a data-toggle="pill" href="#preferences">Preferences</a></li>
      <li><a data-toggle="pill" href="#menu2">Menu 2</a></li>
      <li><a data-toggle="pill" href="#menu3">Menu 3</a></li>
    </ul>
</div>
    
<div class="tab-content col-sm-9">
  <div id="profile" class="tab-pane fade in active">
    <h3>Student Profile</h3>
    <p>Name: {{user.name}}</p>
    <p>Email: {{user.contact_info.email}}</p>
    <p>Website: {{user.contact_info.website1}}</p>
    {{#if user.contact_info.website2}}
      <p>Website: {{user.contact_info.website1}}</p>
    {{/if}}
  </div>
  <div id="edit" class="tab-pane fade">
    <h3>Edit Profile</h3>
  </div>
  <div id="preferences" class="tab-pane fade">
    <h3>Preferences</h3>
  </div>
  <div id="menu2" class="tab-pane fade">
    <h3>Menu 2</h3>
  </div>
  <div id="menu3" class="tab-pane fade">
    <h3>Menu 3</h3>
  </div>
</div>